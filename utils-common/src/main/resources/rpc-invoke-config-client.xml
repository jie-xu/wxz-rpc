<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:nettyrpc="http://www.wxz.com/rpc" xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
    http://www.wxz.com/rpc http://www.wxz.com/rpc/rpc.xsd">

    <context:property-placeholder location="classpath:rpc-server.properties"/>

    <nettyrpc:reference id="addCalculate" interfaceName="com.github.wxz.rpc.service.AddCalculate"
                        protocol="PROTO_STUFF_SERIALIZE" ipAddress="${rpc.server.address}"/>


</beans>
